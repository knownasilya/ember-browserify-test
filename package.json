{
  "name": "ember-commonjs",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "postinstall": "bower install",
    "start": "goat -e index.html",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "browserify -e index.js -o bundle.js",
    "build2": "browserify -e index.js --noparse ember-data -o build.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "bower": "^1.3.9",
    "browserify": "^4.2.3",
    "browserify-shim": "^3.6.0",
    "goat": "^0.3.0"
  },
  "dependencies": {
    "handlebars": "^1.3.0",
    "jquery": "^2.1.1"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browser": {
    "ember": "./bower_components/ember/ember.js",
    "ember-data": "./bower_components/ember-data/ember-data.js"
  },
  "browserify-shim": {
    "ember": {
      "exports": "Ember",
      "depends": [
        "jquery:jQuery",
        "handlebars"
      ]
    },
    "ember-data": {
      "exports": "DS",
      "depends": [
        "ember"
      ]
    }
  }
}
