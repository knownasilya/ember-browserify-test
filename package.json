{
  "name": "ember-commonjs",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "postinstall": "bower install && npm run fix",
    "start": "goat -e index.html",
    "fix": "rm -rf vender && mkdir vender && derequire < ./bower_components/ember/ember.js > ./vender/ember.js && derequire < ./bower_components/ember-data/ember-data.js > ./vender/ember-data.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "browserify -e index.js -o bundle.js",
    "build2": "browserify -e index.js --noparse ember-data -o bundle.js",
    "build3": "browserify -e index.js --noparse ember,ember-data -o bundle.js",
    "build4": "cat vender/ember.js vender/ember-data.js stub.js > bundle.js",
    "build5": "cat bower_components/ember/ember.js bower_components/ember-data/ember-data.js stub.js > bundle.js"

  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "bower": "^1.3.9",
    "browserify": "^4.2.3",
    "browserify-shim": "^3.6.0",
    "goat": "^0.3.0",
    "derequire": "calvinmetcalf/derequire#declarations"
  },
  "dependencies": {
    "handlebars": "^1.3.0",
    "jquery": "^2.1.1"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browser": {
    "ember": "./vender/ember.js",
    "ember-data": "./vender/ember-data.js"
  },
  "browserify-shim": {
    "ember": {
      "exports": "Ember",
      "depends": [
        "jquery:jQuery",
        "handlebars"
      ]
    },
    "ember-data": {
      "exports": "DS",
      "depends": [
        "ember:Ember"
      ]
    }
  }
}
